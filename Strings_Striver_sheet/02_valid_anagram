//leetcode 242

class Solution {
    public boolean isAnagram(String s, String t) {
        char [] c1 = s.toCharArray();
        char [] c2 = t.toCharArray();
        Arrays.sort(c1);
        Arrays.sort(c2);
        boolean result = Arrays.equals(c1,c2);
        if(result == true){
            return true;
        }
        return false;
    }
}