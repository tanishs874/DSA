package string_git_hub;
import java.util.*;
public class print_lower_case {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String s = sc.nextLine();
		for(int i=0;i<s.length();i++) {
			char c = s.charAt(i);
			if(Character.isLowerCase(c)) {
				System.out.print(c);
			}
		}
	}
}
