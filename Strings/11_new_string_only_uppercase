package string_git_hub;
import java.util.*;
public class newstring_only_uppercase {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String s = sc.nextLine();
		String ns = "";
		for(int i=0;i<s.length();i++) {
			char c = s.charAt(i);
			if(Character.isUpperCase(c)) {
				ns = ns + c;
			}
		}
		System.out.print(ns);
	}
}
